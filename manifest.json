{
  "manifest_version": 3,
  "name": "Interview Igniter",
  "version": "1.0",
  "description": "Generate Tailored Interview Questions from Job Descriptions",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
